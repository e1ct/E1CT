<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.e1ct.mappers.TCodeRepository">

	<select id="onDockHoldingHoldingReason" parameterType="kr.co.e1ct.vo.SearchVo" resultType="kr.co.e1ct.vo.TCodeVo">
		<![CDATA[
			SELECT 
				cd_val,
				cd_nm
			FROM 
				t_code  
			WHERE 
				cd_id = 'D05'  
			ORDER BY cd_val
		]]>
	</select>	
</mapper>